{   
    "fiori": { "draft_compat":true },
    "requires": {
        "multitenancy": true,
        "toggles": true,
        "extensibility": true,
        "[production]": {
            "cloud-logging": true,
            "telemetry": {
              "kind": "to-cloud-logging"
            }
        },
        "cds.xt.ExtensibilityService": {
            "element-prefix": [
                "x_"
            ],
            "extension-allowlist": [
                {
                    "for": [
                        "*"
                    ]
                }
            ]
        },
        "db": {
            "vcap": {
                "label": false,
                "plan": "container"
            },
            "pool": {
                "acquireTimeoutMillis": 10000,
                "min": 0,
                "max": 10,
                "fifo": true
            }
        },
        "auth": {
            "kind": "xsuaa"
        }
    }
}